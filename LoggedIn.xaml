<Window x:Class="antroji_praktika.LoggedIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:antroji_praktika"
        mc:Ignorable="d"
        Title="LoggedIn" Height="450" Width="800">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Visible" RenderTransformOrigin="0.55,0.485" HorizontalAlignment="Left" Width="500" Height="419" VerticalAlignment="Top">
            <StackPanel x:Name="StackPanel1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500" />
        </ScrollViewer>
        <Button x:Name="BtnVisosPrekes" Content="Visos prekės" HorizontalAlignment="Left" Margin="560,63,0,0" VerticalAlignment="Top" Width="147" Height="50" Click="BtnVisosPrekes_Click_1"/>
        <Button x:Name="BtnKrepselis" Content="Krepšelis" HorizontalAlignment="Left" Margin="560,142,0,0" VerticalAlignment="Top" Width="147" Height="50" Click="BtnKrepselis_Click"/>
        <Button x:Name="BtnAtsijungti" Content="Atsijungti" HorizontalAlignment="Left" Margin="560,331,0,0" VerticalAlignment="Top" Width="147" Height="50" Click="BtnAtsijungti_Click"/>
    </Grid>
</Window>
