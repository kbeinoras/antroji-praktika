<Window x:Class="antroji_praktika.Registracija"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:antroji_praktika"
        mc:Ignorable="d"
        Title="Registracija" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TxtSlapyvardis" HorizontalAlignment="Left" Height="40" Margin="318,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="294" FontSize="20"/>
        <Label Content="Slapyvardis:" HorizontalAlignment="Left" Margin="170,101,0,0" VerticalAlignment="Top" Height="40" Width="143" FontSize="20"/>
        <Label Content="Slaptažodis:" HorizontalAlignment="Left" Margin="170,197,0,0" VerticalAlignment="Top" Height="40" Width="143" FontSize="20"/>
        <PasswordBox x:Name="PassBox" HorizontalAlignment="Left" Margin="318,197,0,0" VerticalAlignment="Top" Height="40" Width="294"/>
        <Button x:Name="BtnReg" Content="Registruotis" HorizontalAlignment="Left" Margin="305,280,0,0" VerticalAlignment="Top" Width="204" Height="50" Click="BtnReg_Click"/>
        <Button x:Name="BtnAtgal" Content="Atgal" HorizontalAlignment="Left" Margin="674,349,0,0" VerticalAlignment="Top" Width="108" Height="60" Click="BtnAtgal_Click"/>

    </Grid>
</Window>
